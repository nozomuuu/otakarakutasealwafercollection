[http]
	postBuffer = 524288000
	lowSpeedLimit = 0
	lowSpeedTime = 999999
[pack]
	windowMemory = 100m
	packSizeLimit = 100m
	threads = 1
[core]
	compression = 9
[lfs]
	concurrenttransfers = 1
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
